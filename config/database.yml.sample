default: &default
  adapter: postgresql
  encoding: unicode
  username: <%= ENV['DB_USERNAME'] %>
  password: <%= ENV['DB_PASSWORD'] %>
  host: <%= ENV['DB_HOST'] %>
  pool: 5

development:
  <<: *default
  database: blast_stn_knowledge_development

test:
  <<: *default
  database: blast_stn_knowledge_test

production:
  <<: *default
  database: blast_stn_knowledge_production



